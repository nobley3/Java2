package java2prj1.day3.stream_practice;

public class Customer {
// id, name, grade , point grade
	
	String id;
	String name;
	int point;
	
	public Customer() {
		// TODO Auto-generated constructor stub
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getPoint() {
		return point;
	}

	public void setPoint(int point) {
		this.point = point;
	}

	@Override
	public String toString() {
		return "Customer [id=" + id + ", name=" + name + ", point=" + point + "]";
	}

	public Customer(String id, String name, int point) {
		super();
		this.id = id;
		this.name = name;
		this.point = point;
	}
	
	
}
